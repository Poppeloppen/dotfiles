
##### Added aliases #####
#Add color to ls function by default
alias ls='ls -G'

#Make sure the user is prompted before deleting file
alias rm='rm -i'

#Make it easier to navigate files
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .4='cd ../../../..'
alias .5='cd ../../../../..'
alias desk='cd ~/Desktop'
alias 1st='cd ~/Desktop/ITU/1_semester'
alias 2nd='cd ~/Desktop/ITU/2_semester'
alias 3rd='cd ~/Desktop/ITU/3_semester'
alias 4th='cd ~/Desktop/ITU/4_semester'

#Shortcut commands
alias c='clear'
alias h='history'

